{
  "name": "kin-data",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:api && yarn build:web",
    "build:api": "nx build api --prod",
    "build:sdk": "graphql-codegen --config libs/shared/util/sdk/src/codegen.yml",
    "build:web": "nx build web && nx export web",
    "dev:api": "nx serve api",
    "dev:sdk": "yarn build:sdk --watch",
    "dev:web": "nx serve web",
    "docker:build": "docker build . -t kin-data/api",
    "docker:push": "docker push kin-data/api",
    "docker:run": "docker run -it -p 8000:4000 kin-data/api",
    "start": "node dist/apps/api/main.js",
    "test": "nx test"
  },
  "private": true,
  "dependencies": {
    "@google-cloud/bigquery": "^5.9.1",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.6",
    "@kin-tools/circulating-supply": "^1.0.0",
    "@kinecosystem/kin-sdk-v2": "^0.5.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^9.1.1",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/serve-static": "^2.2.2",
    "apollo-link-fragment-argument": "^1.0.1",
    "apollo-server-core": "^3.4.0",
    "apollo-server-express": "^3.4.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "core-js": "^3.6.5",
    "fs-extra": "^10.0.0",
    "graphql": "^15.7.1",
    "joi": "^17.4.2",
    "lru-cache": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "12.9.0",
    "@nrwl/cypress": "13.0.2",
    "@nrwl/eslint-plugin-nx": "12.9.0",
    "@nrwl/jest": "12.9.0",
    "@nrwl/linter": "12.9.0",
    "@nrwl/nest": "12.9.0",
    "@nrwl/node": "12.9.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "12.9.0",
    "@nrwl/web": "13.0.2",
    "@nrwl/workspace": "12.9.0",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "autoprefixer": "^10.3.7",
    "babel-jest": "27.2.3",
    "cypress": "^8.3.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "graphql-tag": "^2.12.5",
    "husky": "^4.3.0",
    "jest": "27.0.3",
    "lint-staged": "^10.4.0",
    "postcss": "^8.3.11",
    "prettier": "^2.3.1",
    "tailwindcss": "^2.2.17",
    "ts-jest": "27.0.3",
    "typescript": "~4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn nx format:check"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html,graphql}": [
      "yarn nx format --uncommitted"
    ]
  }
}
