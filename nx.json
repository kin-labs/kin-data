{
  "npmScope": "kin-data",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "ZWM4NWYzY2EtNDJmMi00NjI4LTk5NDYtMmJkMGE3MTMxYTAxfHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": []
    },
    "api-core-data-access": {
      "tags": ["scope:api", "type:data-access"]
    },
    "api-core-feature": {
      "tags": ["scope:api", "type:feature"]
    },
    "api-core-util": {
      "tags": ["scope:api", "type:util"]
    },
    "api-integration-bigquery": {
      "tags": ["scope:api", "type:util"]
    },
    "api-integration-kin-bi": {
      "tags": ["scope:api", "type:util"]
    },
    "api-stats-data-access": {
      "tags": ["scope:api", "type:data-access"]
    },
    "api-stats-feature": {
      "tags": ["scope:api", "type:data-access"]
    },
    "api-tools-data-access": {
      "tags": ["scope:api", "type:data-access"]
    },
    "api-tools-feature": {
      "tags": ["scope:api", "type:feature"]
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "api",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  }
}
