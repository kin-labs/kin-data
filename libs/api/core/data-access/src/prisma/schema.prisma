generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model DailyActiveUserBalanceByApp {
  id        Int       @id @default(autoincrement()) @map("index")
  date      DateTime? @map("DATE_KEY") @db.Date
  appIndex  BigInt?   @map("APP_ID")
  aub       BigInt?   @map("AUB")
  au        Int?      @map("AU")
  cappedAub BigInt?   @map("CAPPED_AUB")
  appName   String?   @map("NAME")

  @@map("daily_active_user_balance_by_app")
}

model DailyVolatilityFactor {
  id                    Int       @id @default(autoincrement()) @map("index")
  date                  DateTime? @map("DATE_KEY") @db.Date
  averagePrice          Float?    @map("AVERAGE_PRICE")
  totalPriceDeviation   Float?    @map("TOTAL_PRICE_DEVIATION")
  averagePriceDeviation Float?    @map("AVERAGE_PRICE_DEVIATION")
  volatilityFactor      Float?    @map("VOLATILITY_FACTOR")

  @@map("daily_volatility_factor")
}

