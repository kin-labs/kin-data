model DailyKinPayout {
  id                    Int       @id @default(autoincrement()) @map("index")
  date                  DateTime? @map("DATE_KEY") @db.Date
  appIndex              BigInt?   @map("APP_ID")
  appName               String?   @map("APP_NAME")
  preMonopolyShare      Float?    @map("PRE_MONOPOLY_SHARE")
  preVfPayout           Float?    @map("PRE_VF_PAYOUT")
  preMonopolyPayout     Float?    @map("PRE_MONOPOLY_PAYOUT")
  postMonopolyAppShare  Float?    @map("POST_MONOPOLY_APP_SHARE")
  postMonopolyPayout    Float?    @map("POST_MONOPOLY_PAYOUT")
  postMonopolyPayoutUsd Float?    @map("POST_MONOPOLY_PAYOUT_USD")

  @@map("daily_kin_payout")
}

model DailyKinTransactions {
  id                     Int       @id @default(autoincrement()) @map("index")
  date                   DateTime? @map("DATE_KEY") @db.Date
  appIndex               BigInt?   @map("APP_ID")
  appName                String?   @map("APP_NAME")
  dailyTotalTransactions Int?      @map("DAILY_TOTAL_TRANSACTIONS")
  dailyTotalAmounts      Float?    @map("DAILY_TOTAL_AMOUNTS")
  dailyEarnTransactions  Int?      @map("DAILY_EARN_TRANSACTIONS")
  dailyEarnAmounts       Float?    @map("DAILY_EARN_AMOUNTS")
  dailyEarnAverage       Float?    @map("DAILY_EARN_AVERAGE")
  dailyEarnMedian        Float?    @map("DAILY_EARN_MEDIAN")
  dailySpendTransactions Int?      @map("DAILY_SPEND_TRANSACTIONS")
  dailySpendAmounts      Float?    @map("DAILY_SPEND_AMOUNTS")
  dailySpendAverage      Float?    @map("DAILY_SPEND_AVERAGE")
  dailySpendMedian       Float?    @map("DAILY_SPEND_MEDIAN")
  dailyPeerTransactions  Int?      @map("DAILY_PEER_TRANSACTIONS")
  dailyPeerAmounts       Float?    @map("DAILY_PEER_AMOUNTS")
  dailyPeerAverage       String?   @map("DAILY_PEER_AVERAGE")
  dailyPeerMedian        Float?    @map("DAILY_PEER_MEDIAN")

  @@map("daily_kin_transactions")
}

model KreWeeklyPayout {
  id              Int       @id @default(autoincrement()) @map("index")
  date            DateTime? @map("DATE_KEY") @db.Date
  appIndex        BigInt?   @map("APP_ID")
  appName         String?   @map("APP_NAME")
  weeklyKinPayout Float?    @map("WEEKLY_KIN_PAYOUT")
  weeklyUsdPayout Float?    @map("WEEKLY_USD_PAYOUT")
  runDate         DateTime? @map("RUN_DATE") @db.Date
  fromDate        DateTime? @map("FROM_DATE") @db.Date
  toDate          DateTime? @map("TO_DATE") @db.Date
  wallet          String?   @map("WALLET")

  @@map("kre_weekly_payout")
}

